{
  "classId": "idm.component.mobileKnowManage.popupContainer",
  "comName": "弹出层容器",
  "className": "IPopupContainer",
  "comType": "common",
  "comLangue": "vue",
  "compositeAttr": [
    {
      "type": "input",
      "layoutType": "inline",
      "text": "唯一标识",
      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "默认展开",
          "desc": "默认展开或者关闭",
          "bindKey": "isShow",
          "default": true
        },
        {
          "type": "radio",
          "layoutType": "inline",
          "text": "加载模式",
          "desc": "加载容器：可以拖动别的组件拖入，加载目录会默认加载目录",
          "bindKey": "loadType",
          "dictionary": [
              {
                  "label": "加载目录",
                  "value": "loadcatalog"
              },
              {
                  "label": "加载容器",
                  "value": "loadcontainer"
              }
          ],
          "default": "loadcontainer"
        },
        {
          "type": "select",
          "layoutType": "inline",
          "text": "弹出位置",
          "desc": "弹出位置，可以设置为：top、bottom、left、right",
          "bindKey": "showPosition",
          "default": "right",
          "dictionary": [
            {
              "label": "top",
              "value": "top"
            },
            {
              "label": "bottom",
              "value": "bottom"
            },
            {
              "label": "left",
              "value": "left"
            },
            {
              "label": "right",
              "value": "right"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "弹出层样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "inlineGroup",
          "text": "宽高",
          "children": [
            {
              "type": "input",
              "layoutType": "block",
              "text": "宽",
              "desc": "填写auto则为自适应，或者使用px、%、vw等单位，比如100%、100px、100vw等等",
              "bindKey": "boxwidth",
              "width": "60px",
              "default": "80%"
            },
            {
              "type": "input",
              "layoutType": "block",
              "text": "高",
              "desc": "填写auto则为自适应，或者使用px、%、vh等单位，比如100%、100px、100vh等等",
              "bindKey": "boxheight",
              "width": "60px",
              "default": "100%"
            }
          ]
        }
      ]
    },
    {
      "type": "group",
      "text": "内层样式",
      "desc": "点击？查看样式设置用法指南",
      "helpUrl": "",
      "children": [
        {
          "type": "box",
          "layoutType": "block",
          "text": "内外边距",
          "bindKey": "ulbox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "20px",
            "paddingRightVal": "20px",
            "paddingBottomVal": "20px",
            "paddingLeftVal": "20px"
          }
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "标题",
          "desc": "标题字体",
          "bindKey": "tagTitle"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "标题间距",
          "desc": "标题向下的间距",
          "bindKey": "titleBottom",
          "default": "10px"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "目录间距",
          "desc": "目录容器的间距",
          "bindKey": "catalogBottom",
          "default": "20px"
        },
        {
          "type": "box",
          "layoutType": "block",
          "text": "页签边距",
          "bindKey": "tagBox",
          "default": {
            "marginTopVal": "",
            "marginRightVal": "",
            "marginBottomVal": "",
            "marginLeftVal": "",
            "paddingTopVal": "5px",
            "paddingRightVal": "10px",
            "paddingBottomVal": "5px",
            "paddingLeftVal": "10px"
          }
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "页签边框",
          "bindKey": "tagBorder"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "页签字体",
          "desc": "页签字体",
          "bindKey": "tagFont"
        },
        {
          "type": "border",
          "layoutType": "block",
          "text": "选中页签边框",
          "desc": "选中页签边框",
          "bindKey": "chooseTagBorder"
        },
        {
          "type": "font",
          "layoutType": "block",
          "text": "选中页签字体",
          "desc": "选中页签字体",
          "bindKey": "chooseTagFont"
        }
      ]
    },
    {
      "type": "group",
      "text": "高级",
      "desc": "点击？查看高级设置用法指南",
      "children": [
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "请求参数自定义函数",
          "desc": "请求参数自定义函数",
          "bindKey": "customFunc",
          "multiple": false
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "用于选择数据源",
          "bindKey": "dataSource"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "接收消息key",
          "desc": "接收消息key",
          "bindKey": "sendKey",
          "default": "chooseTag"
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "发消息key",
          "desc": "发消息的key",
          "bindKey": "receiveKey",
          "default": "recivekey"
        },
        {
          "type": "pageModuleSelect",
          "layoutType": "block",
          "text": "tag菜单联动组件发送消息",
          "desc": "选择点击tag需要联动的组件",
          "bindKey": "triggerComponents",
          "multiple": true
        },
        {
          "type": "group",
          "text": "字段设置",
          "desc": "数据源接口参数对应的key",
          "children":[
              {
                  "type": "input",
                  "layoutType": "inline",
                  "text": "页签标题",
                  "bindKey": "tabTitleField",
                  "desc": "页签标题字段",
                  "default": "title"
              },
              {
                  "type": "input",
                  "layoutType": "inline",
                  "text": "页签",
                  "bindKey": "tagContentFiled",
                  "desc": "页签内容字段",
                  "default": "list"
              }
          ]
        }
      ]
    }
  ]
}