      "bindKey": "ctrlId",
      "disabled": true,
      "default": "@[packageid]",
      "desc": "",
      "helpUrl": ""
    },
    {
      "type": "group",
      "text": "基本属性",
      "desc": "提供设置组件能达到展示效果的基本属性设置信息",
      "children": [
        {
          "type": "input",
          "layoutType": "inline",
          "text": "占位文字",
          "desc": "搜索栏中的placeholder",
          "bindKey": "placeholder",
          "default": "请输入检索词"
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示容器",
          "desc": "在搜索栏右侧放置一个容器",
          "bindKey": "showDrag",
          "default": true
        },
        {
          "type": "switch",
          "layoutType": "inline",
          "text": "显示热搜",
          "desc": "在搜索栏下显示热搜",
          "bindKey": "showHotWords",
          "default": true
        },
        {
          "type": "input",
          "layoutType": "inline",
          "text": "热搜题目",
          "desc": "热搜题目，为空不显示",
          "bindKey": "hotWordsTitle",
          "display": "@[showHotWords]",
          "default": "热门搜索："
        },
        {
          "type": "dataSourceSelect",
          "layoutType": "inline",
          "text": "数据源",
          "desc": "用于选择热搜的数据源",
          "bindKey": "dataSource",
          "display": "@[showHotWords]"
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "搜索自定义函数",
          "bindKey": "handleSearch",
          "desc": "当搜索时触发此函数，此函数的参数为{commonParam: 组件信息, customParam: 用户自定义参数, value: 搜索的文字}",
          "multiple": false
        },
        {
          "type": "actionSelect",
          "layoutType": "block",
          "text": "热搜自定义函数",
          "bindKey": "handleHotWordClick",
          "desc": "当点击热搜时会触发此函数，此函数的参数为{commonParam: 组件信息, customParam: 用户自定义参数, value: 热搜的文字}",
          "multiple": false,
          "display": "@[showHotWords]"
        }
      ]
    },
